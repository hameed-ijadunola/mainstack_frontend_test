{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "none",
        "semi": true,
        "singleQuote": true,
        "useTabs": false,
        "endOfLine": "auto"
      }
    ]
  }
}
